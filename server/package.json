{
  "name": "csgo-matchmaking-server",
  "version": "0.0.0",
  "description": "NPM packages for csgo-matchmaking-server",
  "private": true,
  "scripts": {
    "prestart": "npm install",
    "start": "node server.js",
    "predev": "npm install && mocha -R spec --recursive",
    "dev": "nodemon -L --watch ./ ./server.js",
    "test": "mocha -R spec --recursive --watch",
    "build": "pkg ./server.js --targets latest-linux-x64,latest-macos-x64,latest-win-x64 --out-path ./build"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "dependencies": {
    "simple-node-logger": "^0.93.30",
    "node-json-db": "^0.7.3",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "mocha": "^4.0.1",
    "should": "^13.1.1",
    "socket.io-client": "^2.0.3",
    "pkg": "^4.2.5"
  }
}
